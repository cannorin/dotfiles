{
    "editor.fontFamily": "Inconsolata",
    "FSharp.msbuildHost": ".net core",
    "FSharp.msbuildAutoshow": true,
    "window.titleBarStyle": "custom",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        }
    ],
    "FSharp.resolveNamespaces": false,
    "explorer.confirmDragAndDrop": false,
    "FSharp.externalAutocomplete": false,
    "editor.minimap.maxColumn": 30,
    "window.zoomLevel": 0,
    "editor.fontSize": 16,
    "editor.fontLigatures": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "window.enableMenuBarMnemonics": false,
    "FSharp.fsacRuntime": "netcore",
    "workbench.editor.tabSizing": "shrink",
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-e",
          "$latex=q/uplatex %O -synctex=1 -interaction=nonstopmode -file-line-error %S/",
          "-e",
          "$bibtex=q/upbibtex %O %B/",
          "-e",
          "$biber=q/biber %O --bblencoding=utf8 -u -U --output_safechars %B/",
          "-e",
          "$makeindex=q/upmendex %O -o %D %S/",
          "-e",
          "$dvipdf=q/dvipdfmx %O -o %D %S/",
          "-norc",
          "-pdfdvi",
          "%DOC%"
        ]
      }
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "editor.tabSize": 2,
    "editor.tabCompletion": "on",
    "FSharp.linter": false,
    "explorer.confirmDelete": false,
    "csharp.suppressDotnetInstallWarning": true,
    "omnisharp.useGlobalMono": "always",
    "FSharp.enableTouchBar": false,
    "latex-workshop.message.update.show": false,
    "latex-workshop.bind.enter.key": false,
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
    "editor.detectIndentation": false,
    "aws.telemetry": false,
    "aws.profile": "default",
    "files.associations": {
      "*.tidal": "haskell"
    },
    "FSharp.enableReferenceCodeLens": false,
    "FSharp.useSdkScripts": true,
    "ocaml.lsp.path": "/home/alice/codes/work/ocaml-lsp/_build/install/default/bin/ocamllsp",
    "ocaml-language-server.trace.server": "messages",
    "FSharp.trace.server": "messages",
    "window.restoreWindows": "one",
    "tidalcycles.sounds.paths": [
      "/home/alice/.local/share/SuperCollider/downloaded-quarks/Dirt-Samples"
    ],
    "java.semanticHighlighting.enabled": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "java.refactor.renameFromFileExplorer": "autoApply",
}
