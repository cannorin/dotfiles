{
  "[markdown]": {
    "editor.fontFamily": "'HackGen Console NF', Consolas, monospace"
  },
  "csharp.suppressDotnetInstallWarning": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'HackGen35 Console NF', Consolas, monospace",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 30,
  "editor.minimap.renderCharacters": false,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.tidal": "haskell"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "FSharp.enableReferenceCodeLens": false,
  "FSharp.enableTouchBar": false,
  "FSharp.externalAutocomplete": false,
  "FSharp.fsacRuntime": "netcore",
  "FSharp.linter": false,
  "FSharp.msbuildAutoshow": true,
  "FSharp.msbuildHost": ".net core",
  "FSharp.resolveNamespaces": false,
  "FSharp.trace.server": "messages",
  "FSharp.useSdkScripts": true,
  "git.openRepositoryInParentFolders": "never",
  "latex-workshop.bind.enter.key": false,
  "latex-workshop.latex.tools": [
    {
      "args": [
        "-e",
        "$latex=q/uplatex %O -synctex=1 -interaction=nonstopmode -file-line-error %S/",
        "-e",
        "$bibtex=q/upbibtex %O %B/",
        "-e",
        "$biber=q/biber %O --bblencoding=utf8 -u -U --output_safechars %B/",
        "-e",
        "$makeindex=q/upmendex %O -o %D %S/",
        "-e",
        "$dvipdf=q/dvipdfmx %O -o %D %S/",
        "-norc",
        "-pdfdvi",
        "%DOC%"
      ],
      "command": "latexmk",
      "name": "latexmk"
    }
  ],
  "latex-workshop.message.error.show": false,
  "latex-workshop.message.warning.show": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "redhat.telemetry.enabled": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "after": [],
      "before": [
        "u"
      ],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "after": [],
      "before": [
        "<C-r>"
      ],
      "commands": [
        {
          "command": "redo"
        }
      ]
    }
  ],
  "window.enableMenuBarMnemonics": false,
  "window.restoreWindows": "one",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.editor.tabSizing": "shrink",
  "quarkus.tools.alwaysShowWelcomePage": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "workbench.colorTheme": "Default Light Modern"
}