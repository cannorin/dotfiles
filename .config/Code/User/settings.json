{
    "editor.fontFamily": "Ricty Diminished",
    "FSharp.lineLens.enabled": "replaceCodeLens",
    "FSharp.msbuildHost": ".net core",
    "FSharp.msbuildAutoshow": true,
    "FSharp.minimizeBackgroundParsing": true,
    "window.titleBarStyle": "custom",
    "FSharp.logLanguageServiceRequestsOutputWindowLevel": "ERROR",
    "FSharp.logLanguageServiceRequests": "none",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        }
    ],
    "FSharp.resolveNamespaces": false,
    "explorer.confirmDragAndDrop": false,
    "FSharp.externalAutocomplete": false,
    "editor.minimap.maxColumn": 30,
    "window.zoomLevel": -0.5,
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.enabled": false,
    "editor.smoothScrolling": true,
    "editor.cursorSmoothCaretAnimation": true,
    "window.enableMenuBarMnemonics": false,
    "FSharp.fsacRuntime": "netcore",
    "workbench.editor.tabSizing": "shrink",
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-e",
          "$latex=q/uplatex %O -synctex=1 -interaction=nonstopmode -file-line-error %S/",
          "-e",
          "$bibtex=q/upbibtex %O %B/",
          "-e",
          "$biber=q/biber %O --bblencoding=utf8 -u -U --output_safechars %B/",
          "-e",
          "$makeindex=q/upmendex %O -o %D %S/",
          "-e",
          "$dvipdf=q/dvipdfmx %O -o %D %S/",
          "-norc",
          "-pdfdvi",
          "%DOC%"
        ]
      }
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "FSharp.disableInMemoryProjectReferences": true,
    "FSharp.enableBackgroundSymbolCache": true,
    "editor.tabSize": 2,
    "editor.tabCompletion": "on",
    "FSharp.linter": false,
    "explorer.confirmDelete": false,
    "csharp.suppressDotnetInstallWarning": true,
    "omnisharp.useGlobalMono": "always",
    "FSharp.enableTouchBar": false,
    "fsharp.trace.server": "messages",
    "latex-workshop.message.update.show": false,
    "latex-workshop.bind.enter.key": false,
    "latex-workshop.message.error.show": false,
    "latex-workshop.message.warning.show": false,
}
